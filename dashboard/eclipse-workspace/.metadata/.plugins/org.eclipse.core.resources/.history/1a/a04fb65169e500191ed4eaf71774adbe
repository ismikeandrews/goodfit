package br.com.projeto.test;

import javax.swing.JOptionPane;

import br.com.projeto.beans.Candidato;
import br.com.projeto.dao.CandidatoDAO;


public class CadastroCandidatoDAO {
	public static void main(String[] args) {
		try {
			CandidatoDAO dao = new CandidatoDAO();;
			Candidato cand = new Candidato();
			
			cand.setNome("Nome");
			cand.setCpf("CPF");
			cand.setRg("RG");
			cand.setDataNasc("Data de Nascimento");
			cand.setDescricao("");
			cand.setCodUsuario(Integer.parseInt(JOptionPane.showInputDialog("Digite o codigo de usuario")));
			
			dao.addCandidato(cand);
			
		}catch(Exception e) {
			e.printStackTrace();
		}finally {
			try {
				
			}catch(Exception e) {
				e.printStackTrace();
			}
		}

	}
}
