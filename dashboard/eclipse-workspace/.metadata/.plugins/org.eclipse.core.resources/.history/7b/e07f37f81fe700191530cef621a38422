package br.com.projeto.teste;

import javax.swing.JOptionPane;

import br.com.projeto.beans.Candidato;
import br.com.projeto.beans.Moderador;
import br.com.projeto.dao.CandidatoDAO;
import br.com.projeto.dao.ModeradorDAO;
import br.com.projeto.dao.UsuarioDAO;

public class CadastroModeradorDAO {

	public static void main(String[] args) {
		CandidatoDAO dao = null;
		Candidato Candidato = null;
		
		try {
			dao = new CandidatoDAO();
			Candidato = new Candidato();
			
			UsuarioDAO usuDAO = new UsuarioDAO();
			
			Candidato.setRazaoSocialCandidato(JOptionPane.showInputDialog("Razao Social"));
			Candidato.setNomeFantasiaCandidato(JOptionPane.showInputDialog("Nome Fantasia"));
			Candidato.setCnpjCandidato(JOptionPane.showInputDialog("Cnpj Candidato"));
			Candidato.setUsuario(usuDAO.getUsuario(Integer.parseInt(JOptionPane.showInputDialog("Digite o codigo do usuario"))));
			
			dao.addCandidato(Candidato);
		}catch(Exception e) {
			e.printStackTrace();
		}finally {
			try {
				
			}catch(Exception e) {
				e.printStackTrace();
			}
		}
	}

}
